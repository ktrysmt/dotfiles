# macOS specific packages
# Usage: brew bundle --file=Brewfile.mac

# macOS CLI tools
brew "util-linux", link: :force
brew "duckdb"
brew "lima"
brew "ncurses"
brew "openssl"
brew "reattach-to-user-namespace"
brew "trash"
brew "zsh"
brew "aws-vault"

# Casks (GUI applications)
cask_args appdir: "/Applications"

cask "alt-tab"
cask "appcleaner"
cask "clibor"
cask "flux-app"
cask "karabiner-elements"
cask "linearmouse"
cask "macgesture"
cask "raycast"
cask "rectangle"
cask "vagrant"
cask "virtualbox"
cask "visual-studio-code"
cask "1password-cli"
cask "cursor"
cask "devtoys"
cask "rancher"
cask "wezterm"
cask "wordservice"
cask "firefox"
